<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault async="false" continueOnError="false" enabled="true" name="RequestVerbFault">
  	<FaultResponse>
        <Set>
            <Payload contentType="application/json">\{"Service Call Error": "Invalid method."}</Payload>
            <StatusCode>405</StatusCode>
            <ReasonPhrase>Requested Method is Invalid</ReasonPhrase>
        </Set>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>